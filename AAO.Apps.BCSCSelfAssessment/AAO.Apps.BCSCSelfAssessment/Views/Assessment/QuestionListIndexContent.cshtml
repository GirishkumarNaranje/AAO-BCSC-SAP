<script>
$(document).ready(function () {
    // Use event delegation and ensure the event handler is attached only once
    $(document).off('click', '#bcsc-excerpt_a').on('click', '#bcsc-excerpt_a', function (event) {
        event.preventDefault(); // Prevent the default action

        $.ajax({
            data: {
                questionId: @(Model.questionId),
                examId: @(Model.examId),
                userid: @(Model.userId),
            },
            url: '/Assessment/GetExcerpt',
            async: false,
            success: function (response) {
                var $accordion = $('#accordion');
                var $htmlContent = $('<div></div>');

                // Clear the existing content in the accordion
                $accordion.empty();

                response.data.ExcerptHeadings.forEach(function (excerptHeading) {
                    var excerptId = excerptHeading.HeadingId;
                    var excerptText = excerptHeading.HeadingText;
                    var excerpts = excerptHeading.Excerpts;

                    var $panel = $('<div class="panel panel-default notes-panel" excerptid="' + excerptId + '"></div>');
                    var $panelHeading = $('<div class="panel-heading notes-panel-bg" role="tab"></div>');
                    var $panelTitle = $('<div class="panel-title view-title clearfix"></div>');
                    var $accordionLink = $('<a role="button" data-toggle="collapse" class="accordian-tab notes-accordian-tab" data-parent="#accordion" href="#' + excerptId + 'collapseOne" aria-expanded="false" aria-controls="' + excerptId + 'collapseOne" excerptid="' + excerptId + '"></a>');
                    var $questionDiv = $('<div id="quesText' + excerptId + '" class="question xs-padleft50 font-bold"></div>');
                    var $questionTextDiv = $('<div id="quesTextDiv' + excerptId + '">' + excerptText + '</div>');
                    var $questionNumber = $('<li class="question-number-view-exam-details_Excerpt" style="list-style-type:none;margin-top:-20px;"><i class="fa fa-lg fa-angle-up expand"><span class="expand"> expand</span></i></li>');

                    $panel.append($panelHeading);
                    $panelHeading.append($panelTitle);
                    $panelTitle.append($accordionLink);
                    $accordionLink.append($questionDiv);
                    $questionDiv.append($questionTextDiv);
                    $questionTextDiv.append($questionNumber);

                    var $panelCollapse = $('<div id="' + excerptId + 'collapseOne" class="panel-collapse collapse in" aria-labelledby="' + excerptId + 'headingOne" aria-expanded="false" excerptid="' + excerptId + '"></div>');
                    var $panelBody = $('<div class="panel-body"></div>');
                    var $nestedAccordion = $('<div class="accordion nested-accordion" id="' + excerptId + 'accordion"></div>');
                    var $accordionGroup = $('<div class="accordion-group"></div>');
                    var $accordionHeading = $('<div class="accordion-heading"></div>');
                    var $containerDiv = $('<div id="' + excerptId + 'ContainerDiv">' + excerpts + '</div>');

                    $panel.appendTo($htmlContent);
                    $panelCollapse.appendTo($htmlContent);
                    $panelCollapse.append($panelBody);
                    $panelBody.append($nestedAccordion);
                    $nestedAccordion.append($accordionGroup);
                    $accordionGroup.append($accordionHeading);
                    $accordionHeading.append($containerDiv);
                });

                $accordion.html($htmlContent);

                if (response.data.ExcerptHeadings.length <= 1) {
                    $(".panel-collapse").each(function () {
                        $(this).removeClass("in");
                    });
                }

                $(document).off('click', '.accordian-tab').on('click', '.accordian-tab', function () {
                    var $icon = $(this).find('.question-number-view-exam-details_Excerpt i');
                    var isExpanded = $(this).attr('aria-expanded') === 'true';

                    if (isExpanded) {
                        if ($icon.hasClass('fa-angle-up')) {
                            $icon.removeClass('fa-angle-up').addClass('fa-angle-right');
                        } else if ($icon.hasClass('fa-angle-right')) {
                            $icon.removeClass('fa-angle-right').addClass('fa-angle-right');
                        }
                    } else {
                        $icon.removeClass('fa-angle-right').addClass('fa-angle-up');
                    }
                });

                $('div._idGenObjectStyleOverride-1 img').addClass('thumbnail');
                $('img[class^="_idGenObjectAttribute"]').addClass('thumbnail');

                $('div._idGenObjectStyleOverride-1 .thumbnail').each(function () {
                    $(this).after('<div class="zoom_icon"><i class="glyphicon glyphicon-zoom-in text-right"></i></div>');
                });

                $('img[class^="_idGenObjectAttribute"].thumbnail').each(function () {
                    var zoomIconDiv = $('<div class="zoom_icon"><i class="glyphicon glyphicon-zoom-in text-right"></i></div>');
                    $(this).after(zoomIconDiv);
                });

                $('div._idGenObjectStyleOverride-1').on('click', function () {
                    var imgElement = $(this).find('img');
                    var lowResImage = imgElement.attr('src');
                    var highResImage = appendSuffix(lowResImage, '_O');
                    showModal(highResImage);
                });

                $('img[class^="_idGenObjectAttribute"]').on('click', function (event) {
                    var lowResImage = $(this).attr('src');
                    var highResImage = appendSuffix(lowResImage, '_O');
                    showModal(highResImage);
                    event.stopPropagation();
                });

                $('.zoom-icon').on('click', function (event) {
                    var lowResImage = $(this).prev().attr('src');
                    var highResImage = appendSuffix(lowResImage, '_O');
                    showModal(highResImage);
                    event.stopPropagation();
                });

                function appendSuffix(imageSrc, suffix) {
                    return imageSrc.replace(/(\.jpg|\.png|\.tif|\.tiff)$/, suffix + '$1');
                }

                function showModal(imageSrc) {
                    $('#showimg').attr('src', imageSrc);
                    $('#myModal').modal('show');
                }

                $('.accordian-tab').trigger('click');

                // Manually switch the tab after AJAX success
                $('#bcsc-excerpt_a').tab('show');
            }
        });
    });
});
</script>

<script>
    var AssessmentQuestionViewModel = {
        next : '@Html.Raw((int?)Model.nextId)',
        prev : @Html.Raw((int?)Model.previousId),
        examId :  '@Html.Raw((int?)Model.examId)',
        currentQuestionId : @Html.Raw((int?)Model.questionId),
        examAttemptId: @Html.Raw((int?)Model.examAttemptId),
        markedStatus : (@Html.Raw((bool?)Model.markCheck).ToString().ToLower()),
        examMode : (@Html.Raw((bool?)Model.examMode).ToString().ToLower()),
        examAnswerToShow : (@Html.Raw((bool?)Model.examAnswerToShow).ToString().ToLower()),
        examStatus: @Html.Raw((int?)Model.examStatus),
        rightChoiceId: 0,
        choiceList: @(Html.Raw(Json.Encode(@Model.choiceList))),
        isYourAnswerCorrect:'@Html.Raw((int?)Model.examAttemptAnswerDTO.IsYourAnswer)',
        examTimeType: (@Html.Raw((bool?)Model.examTimeType).ToString().ToLower()),
        friendlyId: @Html.Raw((int?)Model.FriendlyId),
        topicName: '@Html.Raw((string)Model.TopicName)',
        examType: @Html.Raw((int?)Model.ExamType),
        choiceId: '@Html.Raw((int?)Model.examAttemptAnswerDTO.ChoiceId)',
        isRightId:(@Html.Raw((bool?)Model.examAttemptAnswerDTO.IsRight).ToString().ToLower()),
        PeerUserType:'@Html.Raw((int?)Model.PeerDetails[0].UserType)',
    };
    $(document).ajaxError(function (xhr, props) {
        if (props.status === 401) {
            /**/
            window.location.href = '/Login/SignOut';
        }
    });
    $(document).ready(function () {

        $('.responsive-tabs').responsiveTabs({
            accordionOn: ['xs', 'sm']
        });

        $('a[href^="http://"], a[href^="https://"]').click(function(e){
            var decodedUri=decodeURI(this.href);
            decodedUri= decodedUri.replace(/[^\x20-\x7E]/g, '');
            $(this).attr("href", decodedUri);
            $(this).attr('target','_blank');
        });
        // $('a[href^="http://"], a[href^="https://"]').not('a[href*=gusdecool]').attr('target', '_blank');

        if(parseInt($("input[name='ChoiceAnswer']:checked").val()) > 0) {
            $("#nextID").removeClass("hidden");
            $("#nextID").addClass("visible");
        }
    });

    var relatedContentsValue = @Html.Raw(Json.Encode(Model.RelatedContents));
    $(document).ready(function () {
        if (relatedContentsValue == null || relatedContentsValue == "")
        {
            $('#relatedContents_tab, #relatedContents_tab_a').hide();
        }
    });

</script>
